<%@include file="../../../_include/_include.jsp"%>
<h:form id="formDeleteFunction">
	<t:div style="width:100%;">
		<e:panelGrid columns="2" columnClasses="col1UnDemi,col2UnDemi"
			styleClass="tableJustWidthMax">
			<e:text value="#{msgs['DOMAIN.CONFIRM.DELETE']}">
				<f:param value="#{paramDomainFormationController.domain.ren1DomaineAnnuForm.codDom}"/>
			</e:text>
			<t:panelGroup>
				<e:commandButton value="#{msgs['_.BUTTON.YES']}"
					action="#{paramDomainFormationController.delete}"/>
				<e:commandButton value="#{msgs['_.BUTTON.NO']}"
					action="#{paramDomainFormationController.reset}"
					immediate="true"/>
			</t:panelGroup>
		</e:panelGrid>
	</t:div>
</h:form>
