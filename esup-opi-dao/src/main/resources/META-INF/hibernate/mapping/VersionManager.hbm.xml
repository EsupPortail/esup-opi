<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping package="fr.univ.rennes1.cri.opiR1.domain.beans">

	<class name="VersionManager" table="b_vers_mana" lazy="false" >
	<cache usage="nonstrict-read-write"/>
	<id name="id"> 
			<generator class="native" >
				<param name="sequence">b_vers_mana_seq</param>
			</generator>
		</id> 
	<property name="version" column="vers" />
	</class>	
</hibernate-mapping>


