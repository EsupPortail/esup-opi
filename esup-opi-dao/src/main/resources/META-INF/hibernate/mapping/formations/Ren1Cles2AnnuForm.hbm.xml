<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="fr.univ.rennes1.cri.apogee.domain.beans.Ren1Cles2AnnuForm" table="REN1_CLES2_ANNU_FORM" >
        <composite-id name="id" class="fr.univ.rennes1.cri.apogee.domain.beans.Ren1Cles2AnnuFormId">
            <key-property name="codLang" type="string">
                <column name="COD_LANG" length="3" />
            </key-property>
            <key-property name="codCles" type="string">
                <column name="COD_CLES" length="10" />
            </key-property>
        </composite-id>
       
        <many-to-one name="ren1ClesAnnuForm" class="fr.univ.rennes1.cri.apogee.domain.beans.Ren1ClesAnnuForm" column="COD_CLES" update="false" insert="false" fetch="select" />
       
       <property name="libCles" type="string">
            <column name="LIB_CLES" length="50" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
