<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
    package="org.esupportail.opi.domain.beans.references.commission"
>
    <class name="Commission" table="commission">
        <id name="id">
            <column name="id_cmi" not-null="true" sql-type="integer" />
            <generator class="native">
                <param name="sequence">commission_seq</param>
            </generator>
        </id>
        <property column="cod_cmi" generated="never" lazy="false"
            length="10" name="code" not-null="true" />
        <property column="lib_cmi" generated="never" lazy="false"
            length="60" name="libelle" not-null="true" />
        <property column="date_crea_enr_cmi" generated="never"
            lazy="false" name="dateCreaEnr" />
        <property column="date_modif_enr_cmi" generated="never"
            lazy="false" name="dateModifEnr" />
        <property column="cod_user_to_create_cmi" generated="never"
            lazy="false" name="codUserToCreate" />
        <property column="cod_user_to_update_cmi" generated="never"
            lazy="false" name="codUserToUpdate" />
        <property column="tem_en_sve_cmi" generated="never"
            lazy="false" name="temoinEnService" type="yes_no" />
        <set inverse="true" name="members" sort="unsorted">
            <key column="id_cmi" />
            <one-to-many class="Member" />
        </set>
        <set cascade="all" inverse="true" lazy="true" name="traitementCmi"
            sort="unsorted">
            <key column="id_cmi" />
            <one-to-many class="TraitementCmi" />
        </set>
        <many-to-one cascade="delete" column="id_cal_cmi"
            lazy="no-proxy" name="calendarCmi" />
        <many-to-one
            class="org.esupportail.opi.domain.beans.references.rendezvous.CalendarRDV"
            column="id_rdv_calendar" name="calendrierRdv" />
        <map cascade="delete,delete-orphan" inverse="true" lazy="true"
            name="contactsCommission" sort="unsorted" table="contactComm">
            <key column="id_cmi" />
            <map-key column="cod_ri_coord" type="integer" />
            <one-to-many class="ContactCommission" />
        </map>
    </class>
</hibernate-mapping>
